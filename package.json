{
  "name": "nah-key-scout",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel --filter=web",
    "build": "turbo run build",
    "start": "pnpm --filter web start",
    "lint": "turbo run lint",
    "lint:fix": "turbo run format",
    "format": "turbo run format",
    "typecheck": "turbo run typecheck",
    "revalidate": "turbo run revalidate --filter=cli",
    "opensea-listings": "turbo run opensea:listings --filter=cli"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "turbo": "^2.0.0"
  }
}
